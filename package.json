{
  "name": "@mcollis/remote-data",
  "version": "1.1.0",
  "description": "RemoteData ADT with react hook",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "preexample": "cd example && npm ci",
    "example": "cd example && npm start",
    "prestart": "npm run build",
    "start": "npx npm-run-all -p \"build -- -w\" example",
    "clean": "npx rimraf dist",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "lint": "eslint --ext jsx --ext js ./src ./example/src",
    "test": "jest",
    "prepare": "npm run build",
    "preversion": "git push",
    "postversion": "git push --tags"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-url": "^4.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.3.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "folktale": "^2.3.2"
  },
  "dependencies": {
    "folktale": "^2.3.2",
    "ramda": "^0.28.0"
  },
  "directories": {
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcollis/remote-data.git"
  },
  "bugs": {
    "url": "https://github.com/mcollis/remote-data/issues"
  },
  "homepage": "https://github.com/mcollis/remote-data#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  }
}
